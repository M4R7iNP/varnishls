sub vcl_recv {
    if (req.url ~ "/a/") {
        call a;
    } else if (req.url ~ "/b/") {
        call b;
    } else if (req.url ~ "/c/") {
        call c;
    } else if (req.url ~ "/d/") {
        call d;
    } else {
        call e;
    }
}
